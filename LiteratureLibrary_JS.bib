  doc-delivery-number        = {604DP},
  doi                        = {10.1672/0277-5212(2002)022{[}0478:DGASOP]2.0.CO;2},
  file                       = {:Guilloy-Froget2002.pdf:PDF},
  groups                     = {Dissertation},
