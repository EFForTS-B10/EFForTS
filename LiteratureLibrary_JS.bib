@Article{VandenBroeck2005,
  author    = {{V}anden {B}roeck, {A}. and {V}illar, {M}. and {V}an {B}ockstaele, {E}. and {V}an {S}lycken, {J}.},
  title     = {{N}atural hybridization between cultivated poplars and their wild relatives: evidence and consequences for native poplar populations},
