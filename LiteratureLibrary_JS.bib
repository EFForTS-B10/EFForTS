  de        = {landscape model; effective distance; cost-distance; connectivity;EOLEOLisolation; environmental planning},
  doi       = {10.1016/S0169-2046(02)00242-6},
  file      = {:Adriaensen2003.pdf:PDF},
  ga        = {700LN},
