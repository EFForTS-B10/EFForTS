@Article{Adlard1995,
  author    = {{A}dlard, {P}.{G}.},
  title     = {{M}yth and reality in growth estimation},
  journal   = {Forest Ecology and Management},
  year      = {1995},
  volume    = {71},
  pages     = {171--176},
  abstract  = {The nature of growth data on which models have depended for their development and validation is reviewed. The purposes for which growth models are developed include growth and yield prediction, health monitoring, long term productivity monitoring and socio-economic analyses of forest influences. The range of models now available for these purposes range from simple statistical stand models to those of physiological processes within a single tree. Tree and stand models of an empirical nature are likely to serve the needs of management in this decade, at least, but the trend to mixed ‘phenomenological’ models will increase. The problems encountered in obtaining reliable data of long-term growth trends lead to the need for continuity, consistency, and constraint in data collection.},
  file      = {:Adlard1995.pdf:PDF},
  groups    = {Dissertation},
  keywords  = {Growth modelling; Growth estimation; Yield estimation; Health monitoring; Productivity monitoring; Socioeconomic analysis},
  owner     = {Jan},
  timestamp = {2014.07.04},
}

@Article{Adler1996,
  author    = {{A}dler, {F}.{R}.},
  title     = {{A} model of self-thinning through local competition},
  journal   = {Proceedings of the National Academy of Sciences of the United States of America},
  year      = {1996},
  volume    = {93},
  pages     = {9980--9984},
  abstract  = {Explanations of self-thinning in plant populations have focused on plant shape and packing. A dynamic model based on the structure of local interactions successfully reproduces the pattern and can be approximated to identify key parameters and relationships. The approach generates testable new explanations for differences between species and populations, unifies self-thinning with other patterns in plant population dynamics, and indicates why organisms other than plants can follow the law.},
  file      = {:Adler1996.pdf:PDF},
