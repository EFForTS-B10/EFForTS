@Article{Ziegenhagen2008,
  author                     = {{Z}iegenhagen, {B}irgit and {G}neuss, {S}ilke and {R}athmacher, {G}eorg and {L}eyer, {I}lona and {B}ialozyt, {R}onald and {H}einze, {B}erthold and {L}iepelt, {S}ascha},
  title                      = {{A} fast and simple genetic survey reveals the spread of poplar hybrids at a natural {E}lbe river site},
  journal                    = {CONSERVATION GENETICS},
  year                       = {2008},
  volume                     = {9},
  number                     = {2},
  pages                      = {373--379},
  abstract                   = {It is known that various poplar species and cultivated poplar hybrids have the potential to interbreed and produce fertile offspring. Conservation strategies for the genetic resources of the endangered Eurasian black poplar (Populus nigra L.) thus rely on a monitoring which enables the identification and verification of the pure species status. At the same time, the risk of hybrid dispersal and introgressive gene flow has to be estimated. In the present study a combination of two molecular markers, one from chloroplast DNA and the other from nuclear DNA, was applied to evaluate a large P. nigra population on the Elbe River. Hybrid clones of P. x canadensis are scattered within this population and also occur as plantations in the surrounding landscape. By means of the DNA markers the taxonomic status of 208 adult trees in the population and 140 young poplars along the riverbank was monitored. From the analysed young poplars, almost 20 percent were found to exhibit at least one of the two P. deltoides or P. x canadensis diagnostic alleles or genotypes, respectively. Possible vegetative spreads of F1 hybrids and precedent mating scenarios are discussed. Most interestingly we found clear evidence for a small number of backcross hybrids where P. X canadensis acted as pollen donor. This case had long been debated and thought to be less probable, so far.},
  address                    = {VAN GODEWIJCKSTRAAT 30, 3311 GZ DORDRECHT, NETHERLANDS},
