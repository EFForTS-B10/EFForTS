  keywords-plus              = {ENVIRONMENTAL STOCHASTICITY; DEMOGRAPHIC-MODELS; CARRYING-CAPACITY; METAPOPULATION; DYNAMICS; RISK; CONSERVATION},
  language                   = {English},
  number-of-cited-references = {49},
