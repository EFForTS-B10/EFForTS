  keywords-plus              = {POLLEN DISPERSAL; POPULUS-TRICHOCARPA; NATURAL-POPULATIONS; FOREST TREES; PATERNITY; PARENTAGE; GENOME; CROPS; RISK; WILD},
  language                   = {English},
