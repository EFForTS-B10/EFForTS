@Article{Smulders2008,
  author                     = {{S}mulders, {M}. {J}. {M}. and {C}ottrell, {J}. {E}. and {L}efevre, {F}. and van der {S}choot, {J}. and {A}rens, {P}. and {V}osman, {B}. and {T}abbener, {H}. {E}. and {G}rassi, {F}. and {F}ossati, {T}. and {C}astiglione, {S}. and {K}rystufek, {V}. and {F}luch, {S}. and {B}urg, {K}. and {V}ornam, {B}. and {P}ohl, {A}. and {G}ebhardt, {K}. and {A}lba, {N}. and {A}gundez, {D}. and {M}aestro, {C}. and {N}otivol, {E}. and {V}olosyanchuk, {R}. and {P}ospiskova, {M}. and {B}ordacs, {S}. and {B}ovenschen, {J}. and van {D}am, {B}. {C}. and {K}oelewijn, {H}. {P}. and {H}alfmaerten, {D}. and {I}vens, {B}. and van {S}lycken, {J}. and {B}roeck, {A}. {V}anden and {S}torme, {V}. and {B}oerjan, {W}.},
  title                      = {{S}tructure of the genetic diversity in black poplar ({P}opulus nigra {L}.) populations across {E}uropean river systems: {C}onsequences for conservation and restoration},
  journal                    = {FOREST ECOLOGY AND MANAGEMENT},
  year                       = {2008},
  volume                     = {255},
  number                     = {5-6},
  pages                      = {1388--1399},
  month                      = apr,
  abstract                   = {Black poplar (Populus nigra L.) is a keystone species for riparian ecosystems in Europe. We analysed the structure of genetic diversity of 17 populations from I I river valleys that are part of seven catchment systems (Danube, Ebro, Elbe, Po, Rhine, Rhone, and Usk) in Europe, in relation to geography and river management. In total, 1069 trees were genotyped using AFLP and microsatellite markers. The trees had an observed heterozygosity of 0.74 (range 0.59-0.82 across microsatellite loci). The majority (72.6-90.8\%, depending on the marker system) of the genetic variation was present within populations. Most pairs of populations along a river were relatively similar (pairwise F(st), 0.042-0.135 based on AFLP, 0.002-0.037 based on microsatellites). Overall population differentiation among rivers was considerable (F(st) among populations was 0.268 based on AFLR and 0.081 based on microsatellites). An analysis using the program Structure indicated that all populations recruited plants from several clusters. Geographically close populations tended to draw from the same Structure clusters, including populations from adjacent catchments. The Danube and Inn populations in Austria were genetically more similar to the Vltava population (Elbe catchment) in Czech Republic than the geographically more distant populations along the Tisa and Prut rivers of the Danube catchment in Ukraine. This indicates that gene flow and dispersal takes place across fairly large distances and between river catchments. Consistent with this result, a principal coordinate analysis of genetic distances among individual trees based on AFLP bands showed large overlap of populations, although the French and Spanish samples formed distinct clusters, and the samples from the Ticino (Italy) were at an intermediate position. The extent of clonal duplication was highest along regulated rivers, with e.g., 41\% clonal duplication along the Rhine in The Netherlands (up to 32 trees for one genet). The Usk contained a man-made population (two genotypes along the entire river, one genet present as 70 trees out of 72 trees sampled). No clonal duplication was found along dynamic rivers, such as the Ebro (Spain), the Drome (France), and the Tisa and Prut (Ukraine). It is concluded that the restoration of the natural habitat and the re-creation of the natural dynamics of the floodplain, in combination with sufficiently sized and spaced natural populations as seed sources, are the most important measures for sufficient natural regeneration and conservation of this species in the future. (C) 2007 Elsevier B.V. All rights reserved.},
  address                    = {PO BOX 211, 1000 AE AMSTERDAM, NETHERLANDS},
  affiliation                = {Smulders, MJM (Reprint Author), Wageningen UR, Plant Res Int, Biodivers \& Breeding, POB 16, NL-6700 AA Wageningen, Netherlands. Smulders, M. J. M.; van der Schoot, J.; Arens, P.; Vosman, B., Wageningen UR, Plant Res Int, Biodivers \& Breeding, NL-6700 AA Wageningen, Netherlands. Cottrell, J. E.; Tabbener, H. E., Forestry Commiss Roslin, No Res Stn, Roslin EH25 9SY, Midlothian, Scotland. Lefevre, F., INRA, Unite Rech Forestieres Mediterraneennes, F-84000 Avignon, France. Grassi, F.; Fossati, T.; Castiglione, S., Univ Milan, Dept Biol, I-20133 Milan, Italy. Krystufek, V.; Fluch, S.; Burg, K., Austrian Res Ctr, Dept Biotechnol, A-2444 Seibersdorf, Austria. Vornam, B., Univ Gottingen, Inst Forest Genet \& Forest Tree Breeding, D-37077 Gottingen, Germany. Pohl, A.; Gebhardt, K., Hessian Forest Ctr Management Planning Res \& Ecol, D-34346 Hannover Muenden, Germany. Alba, N.; Agundez, D., CIFOR INIA, Dept Breeding \& Biotechnol, Madrid 28080, Spain. Maestro, C.; Notivol, E., Govt Aragon, CITA, Forest Resources Unit, Zaragoza 50080, Spain. Volosyanchuk, R., URIFFM, Dept Tree Breeding, UA-61024 Kharkov, Ukraine. Pospiskova, M., VUKOZ, Pruhonice, Czech Republic. Bordacs, S., OMMI, Budapest, Hungary. Bovenschen, J.; van Dam, B. C.; Koelewijn, H. P., Wageningen UR, NL-6700 AA Wageningen, Netherlands. Halfmaerten, D.; Ivens, B.; van Slycken, J.; Broeck, A. Vanden, Res Inst Nat \& Forest, B-9500 Geraardsbergen, Belgium. Storme, V.; Boerjan, W., Univ Ghent VIB, B-9052 Ghent, Belgium.},
  author-email               = {Rene.smulders@wur.nl},
  doc-delivery-number        = {282WK},
  doi                        = {10.1016/j.foreco.2007.10.063},
  file                       = {:Smulders2008.pdf:PDF},
  groups                     = {Dissertation},
  issn                       = {0378-1127},
  journal-iso                = {For. Ecol. Manage.},
  keywords                   = {AFLP; microsatellite marker; clonal propagation; river dynamics; riparian ecosystem; population genetics},
  keywords-plus              = {FRAGMENT LENGTH POLYMORPHISM; MICROSATELLITE MARKERS; NATURAL-POPULATIONS; AFLP MARKERS; LINKAGE MAPS; DISPERSAL; STANDS; REPRODUCTION; TREMULOIDES; SALICACEAE},
  language                   = {English},
