  file      = {:Martin-Queller2013.pdf:PDF},
  fu        = {Ministerio de Ciencia e Innovacion (Spain); European FEDER funds throughEOLEOLDECOFOR [AGL2009-07140/FOR]; MONTES CONSOLIDER projects [CSD2008-00040]},
  fx        = {This research was financially supported by the Ministerio de Ciencia eEOLEOLInnovacion (Spain) and European FEDER funds through DECOFOREOLEOL(AGL2009-07140/FOR) and MONTES CONSOLIDER projects (CSD2008-00040).},
  ga        = {099XP},
