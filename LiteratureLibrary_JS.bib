  journal-iso                = {Conserv. Genet.},
  keywords                   = {Populus nigra L. center dot P. x canadensis moench; DNA markers; backcrosses; introgressive gene flow; hybrid swarm; conservation},
  keywords-plus              = {POPULUS-NIGRA L.; CULTIVATED POPLARS; CHLOROPLAST DNA; MARKERS; FLOW; INTROGRESSION; MITOCHONDRIAL; HYBRIDIZATION; POPULATIONS; INHERITANCE},
  language                   = {English},
  number-of-cited-references = {24},
