  j9        = {LANDSCAPE URBAN PLAN},
  ji        = {Landsc. Urban Plan.},
  keywords  = {HABITAT FRAGMENTATION; CONNECTIVITY MEASURES; PATCH CONNECTIVITY; METAPOPULATION; LYNX},
