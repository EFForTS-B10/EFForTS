  journal-iso                = {For. Ecol. Manage.},
  keywords                   = {growth and yield; stand growth simulator; single tree-based modeling; model application; model evaluation; validation; implementation},
  keywords-plus              = {FOREST GROWTH; SPRUCE; MODELS; BEECH},
  language                   = {English},
